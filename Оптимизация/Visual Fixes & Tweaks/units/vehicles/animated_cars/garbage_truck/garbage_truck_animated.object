      <object name="c_convex_02" collision_type="convex" padding="-2"/>
      <object name="c_convex_03" collision_type="convex" padding="-2"/>
      <object name="c_convex_04" collision_type="convex" padding="-2"/>
      <object name="c_convex_05" collision_type="convex" padding="-2"/>
      <object name="c_convex_06" collision_type="convex" padding="-2"/>
      <object name="c_convex_07" collision_type="convex" padding="-2"/>
      <object name="c_convex_08" collision_type="convex" padding="-2"/>
      <object name="c_convex_09" collision_type="convex" padding="-2"/>
      <object name="c_convex_10" collision_type="convex" padding="-2"/>
      <object name="c_convex_11" collision_type="convex" padding="-2"/>
	  
      <object name="c_box_1" collision_type="box" padding="-2"/>
      <object name="c_box_2" collision_type="box" padding="-2"/>
      <object name="c_box_3" collision_type="box" padding="-2"/>
      <object name="c_box_4" collision_type="box" padding="-2"/>
      <object name="c_box_5" collision_type="box" padding="-2"/>
      <object name="c_box_6" collision_type="box" padding="-2"/>
	  
      <object name="c_capsule_1" collision_type="capsule" padding="-2"/>
      <object name="c_capsule_2" collision_type="capsule" padding="-2"/>
	  
	  
      <object name="c_left_wheel" collision_type="convex" padding="-2"/>
	  <object name="c_right_wheel" collision_type="convex" padding="-2"/>
	  
	  
    </body>
  </bodies>
  
  <decal_surfaces default_material="metal">
    <decal_mesh name="dm_rubber" enabled="true" material="rubber" />
    <decal_mesh name="dm_plastic" enabled="true" material="plastic" />
    <decal_mesh name="dm_sheetmetal" enabled="true" material="metal_chassis" />
    <decal_mesh name="dm_glas" enabled="true" material="glass_breakable" />
    <decal_mesh name="dm_metal" enabled="true" material="metal_hollow" />
	<decal_mesh name="dm_right_wheel_rubber" enabled="true" material="rubber" />
	<decal_mesh name="dm_right_wheel_sheetmetal" enabled="true" material="sheet_metal" />
	<decal_mesh name="dm_left_wheel_rubber" enabled="true" material="rubber" />
	<decal_mesh name="dm_left_wheel_sheetmetal" enabled="true" material="sheet_metal" />
  </decal_surfaces>
  
  <graphics>
    <object name="g_window" enabled="true" />
    <object name="g_garbagetruck" enabled="true" />
    <object name="g_body" enabled="false" />
    <object name="g_left_wheel" enabled="true" />
    <object name="g_right_wheel" enabled="true" />
  </graphics>
</dynamic_object>