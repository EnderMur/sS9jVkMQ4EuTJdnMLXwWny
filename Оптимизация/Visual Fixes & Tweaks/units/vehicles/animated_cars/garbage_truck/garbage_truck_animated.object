<?xml version="1.0"?>
<dynamic_object>
  <diesel materials="units/vehicles/garbagetruck/garbagetruck" orientation_object="rp_garbagetruck_anim" />
  <sequence_manager file="units/vehicles/animated_cars/garbage_truck/garbage_truck_animated" />
  <animations>
    <animation_group name="animation">
      <object name="a_anim" />
      <object name="a_body" />
      <object name="left_wheel" />
      <object name="right_wheel" />
    </animation_group>
  </animations>
  <bodies>
    <body name="animated_body" enabled="true" template="animated">
	  <object name="a_anim" />
      <object name="a_body" />
