<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/pd2_dlc_bex/vehicles/anim_vehicle_suv_bex/anim_vehicle_suv_bex" orientation_object="rp_car_crash" />
	<sequence_manager file="units/pd2_dlc_bex/vehicles/anim_vehicle_suv_bex/anim_vehicle_suv_bex" />

	<animation_def name="anims/units/bex_suv/bex_suv" />

	<bodies>
		<body name="body_mover_blocker" enabled="false" template="mover_blocker" keyframed="true">
			<object name="anim_body" />
			<object name="c_box_mover" 			collision_type="box" />
			<object name="c_convex_mover" 		collision_type="convex" />
		</body>

		<body name="body_vehicle" enabled="true" template="animated">
			<object name="anim_body" />
			<object name="c_vehicle" collision_type="mesh_mopp" />
			<object name="c_box_ceiling" collision_type="box" padding="-2.5"/>
		</body>

		<body name="body_tire_front_right" enabled="true" template="animated">
			<object name="anim_tire_front_right" />
			<object name="c_tire_front_right" collision_type="mesh_mopp" />
		</body>

		<body name="body_tire_front_left" enabled="true" template="animated">
			<object name="anim_tire_front_left" />
			<object name="c_tire_front_left" collision_type="mesh_mopp" />
		</body>

		<body name="body_tire_rear_right" enabled="true" template="animated">
			<object name="anim_tire_rear_right" />
			<object name="c_tire_rear_right" collision_type="mesh_mopp" />
		</body>

		<body name="body_tire_rear_left" enabled="true" template="animated">
			<object name="anim_tire_rear_left" />
			<object name="c_tire_rear_left" collision_type="mesh_mopp" />
		</body>

	</bodies>

	<decal_surfaces default_material="metal">
		<decal_mesh name="dm_vehicle" 				enabled="true" material="metal_chassis" />
		<decal_mesh name="dm_tire_front_left" 		enabled="true" material="steel" />
		<decal_mesh name="dm_tire_rear_left" 		enabled="true" material="steel" />
		<decal_mesh name="dm_tire_front_right" 		enabled="true" material="steel" />
		<decal_mesh name="dm_tire_rear_right" 		enabled="true" material="steel" />
	</decal_surfaces>

	<graphics>

			<object name="g_vehicle_body" 			enabled="true" shadow_caster="true" />
			<object name="g_grill" 					enabled="true" shadow_caster="true" />
			<object name="g_armor" 					enabled="true" shadow_caster="true" />

			<object name="g_tire_front_left" 		enabled="true" shadow_caster="true" />
			<object name="g_tire_front_left_lod1" 	enabled="false"/>
			<object name="g_tire_front_right" 		enabled="true" shadow_caster="true" />
			<object name="g_tire_front_right_lod1" 	enabled="false"/>
			<object name="g_tire_rear_left" 		enabled="true" shadow_caster="true" />
			<object name="g_tire_rear_left_lod1" 	enabled="false"/>
			<object name="g_tire_rear_right" 		enabled="true" shadow_caster="true" />
			<object name="g_tire_rear_right_lod1" 	enabled="false"/>

			<object name="g_glass" 					enabled="true" shadow_caster="false" />
			<object name="g_mullplan" 				enabled="true"/>
			
			<object name="s_vehicle" 				enabled="true" shadow_caster="true" />
			<object name="s_tire_front_right" 		enabled="true" shadow_caster="false" />
			<object name="s_tire_front_left" 		enabled="true" shadow_caster="false" />
			<object name="s_tire_rear_right" 		enabled="true" shadow_caster="false" />
			<object name="s_tire_rear_left" 		enabled="true" shadow_caster="false" />
		</graphics>

		<graphic_group name="grp_wire" enabled="false" culling_object="co_cull1">
			<lod_object name="lod_vehicle_wire">
				<object name="g_wire" enabled="true" shadow_caster="true">
					<material materials="mtr_wire" max_distance="2500"/>
					<material materials="mtr_wire_lod"/>
				</object>
			</lod_object>
		</graphic_group>

		<object name="g_wire_static" enabled="true"/>
		<graphic_group name="grp_gate" enabled="false" culling_object="co_cull2">
			<object name="g_bars" 					enabled="true" shadow_caster="true" />
		</graphic_group>
	</graphics>
	<lights>
		<light editable="true" name="li_light_blue" multiplier="0.1" far_range="350" falloff="4" enabled="false" type="omni|specular" />
		<light editable="true" name="li_light_red" multiplier="0.1" far_range="350" falloff="4" enabled="false" type="omni|specular" />
	</lights>
</dynamic_object>